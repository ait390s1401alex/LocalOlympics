<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

    <!-- ADMIN SERVLET: Record -->
    
	<servlet>
		<servlet-name>AddRecordServlet</servlet-name>
		<servlet-class>localolympics.servlet.AddRecordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddRecordServlet</servlet-name>
		<url-pattern>/admin/addRecord</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteRecordServlet</servlet-name>
		<servlet-class>localolympics.servlet.DeleteRecordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteRecordServlet</servlet-name>
		<url-pattern>/admin/deleteRecord</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UpdateRecordServlet</servlet-name>
		<servlet-class>localolympics.servlet.UpdateRecordServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateRecordServlet</servlet-name>
		<url-pattern>/admin/updateRecord</url-pattern>
	</servlet-mapping>

	
	
	
	
	
	
	
	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
